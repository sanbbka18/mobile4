<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lab1_Students"
        tools:targetApi="31">
        <activity
            android:name=".StudentActivity"
            android:exported="false"
            android:parentActivityName=".GroupsListActivity" />
        <activity
            android:name=".AddStudentActivity"
            android:exported="false"
            android:parentActivityName=".GroupsListActivity" />
        <activity
            android:name=".AddStudentsGroupActivity"
            android:exported="false"
            android:parentActivityName=".GroupsListActivity" />
        <activity
            android:name=".GroupsListActivity"
            android:exported="false"
            android:label="Список груп"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StudentsGroupActivity"
            android:exported="false" />
        <activity
            android:name=".StudentsListActivity"
            android:exported="false"
            android:label="Список студентів"
            android:parentActivityName=".GroupsListActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>